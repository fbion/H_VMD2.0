<?xml version="1.0" encoding="utf-8" ?>
<mapper>
  <select id="SelectMethod">
    select  * from ds_method
    <where>
      <if type="isNotEmpty"  property="resourceid" >
        resourceid = @resourceid
      </if>
      <if type="isNotEmpty"  property="name" >
       and name = @name
      </if>
    </where>
  </select>
  <select id="SelectContent">
    select  * from ds_content where  id = @id
  </select>
</mapper>