{"vmdversion":"2.0.3.0908","vmdlayout":"{\"components\":[{\"cid\":\"viewport\",\"id\":\"MyViewport\",\"layoutConfig\":{},\"userConfig\":{\"layout\":\"absolute\"},\"cn\":[{\"cid\":\"vmd.ux.ColorSelectInner\",\"id\":\"ColorSelectInner\",\"layoutConfig\":{},\"userConfig\":{\"x\":70,\"y\":20}},{\"cid\":\"vmdButton\",\"id\":\"button\",\"layoutConfig\":{},\"userConfig\":{\"x\":480,\"y\":90,\"id\":\"button\",\"text\":\"取值测试\",\"click\":\"button_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button1\",\"layoutConfig\":{},\"userConfig\":{\"x\":480,\"y\":130,\"text\":\"传值测试\",\"click\":\"button1_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button2\",\"layoutConfig\":{},\"userConfig\":{\"x\":490,\"y\":170,\"text\":\"clear\",\"click\":\"button2_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button3\",\"layoutConfig\":{},\"userConfig\":{\"x\":490,\"y\":210,\"text\":\"JSON\",\"click\":\"button3_click\"}}]},{\"cid\":\"vmddataset\",\"id\":\"数据集\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdvariable\",\"id\":\"变量\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdsubviewport\",\"id\":\"SubView_Window\",\"layoutConfig\":{},\"userConfig\":{}}]}","vmdevents":"function bgColorSetting() {\n    this.ColorSelectInner = {\n        id: \"ColorSelectInner\",\n        value: \"#000000\",\n        type: \"ColorSelectInner\"\n    }\n    //清空属性\n    this.clearInfo = function() {\n        this.ColorSelectInner.value = \"#000000\"\n    };\n\n    //设置值\n    this.setFontInfo = function(info, cell) {\n        this.ColorSelectInner.value = info.ColorSelectInner.value;\n    };\n\n    // 根据id设置相应属性的值\n    this.setCellInfos = function(id, value) {\n        var c = this.getInfoByID(id);\n        if(c) {\n            if(c.type == \"Text\") {\n                c.value = value;\n            }\n            if(c.type == \"CheckBox\") {\n                c.checked = value;\n            }\n            if(c.type == \"ComboBox\") {\n                c.value = value;\n            }\n            if(c.type == \"compound\") {\n                c.value = value;\n            }\n            if(c.type == \"comlist\") {\n                c.value = value;\n            }\n            if(c.type == \"radiostoregroup\") {\n                c.value = value;\n            }\n            if(c.type == \"ColorSelectInner\") {\n                c.value = value;\n            }\n        }\n    };\n\n    //通过id获取属性\n    this.getInfoByID = function(id) {\n        if(this.ColorSelectInner.id == id) {\n            return this.ColorSelectInner;\n        }\n\n    }\n\n    //转换JSON串\n    this.cellInfoToJson = function() {\n\n        json = {\n            background_color: [{\n                ColorSelectInner: {\n                    id: 'ColorSelectInner',\n                    value: this.ColorSelectInner.value,\n                    type: 'ColorSelectInner'\n                }\n            }]\n        }\n\n    }\n}\nvar p = new bgColorSetting();\n\nfunction button_click(sender, e) {\n    p.ColorSelectInner.value = \"#5e5e5e\"\n    ColorSelectInner.setOriColor(p.ColorSelectInner.value);\n    ColorSelectInner.setColorDiv(p.ColorSelectInner.value)\n}\n\n\n\nfunction button1_click(sender, e) {\n    p.ColorSelectInner.value = ColorSelectInner.getSelColor();\n    alert(p.ColorSelectInner.value)\n}\n\nfunction button2_click(sender, e) {\n    p.clearInfo();\n    ColorSelectInner.setOriColor(p.ColorSelectInner.value);\n    ColorSelectInner.setColorDiv(p.ColorSelectInner.value)\n}\n\nfunction button3_click(sender, e) {\n    p.ColorSelectInner.value = ColorSelectInner.getSelColor();\n    p.cellInfoToJson();\n    alert(JSON.stringify(json))\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}