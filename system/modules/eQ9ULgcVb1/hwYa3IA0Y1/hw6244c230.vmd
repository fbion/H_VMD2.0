{"vmdversion":"2.0.3.0908","vmdlayout":"{\"components\":[{\"cid\":\"viewport\",\"id\":\"MainViewport\",\"layoutConfig\":{},\"userConfig\":{\"layout\":\"absolute\"},\"cn\":[{\"cid\":\"vmdButton\",\"id\":\"button\",\"layoutConfig\":{},\"userConfig\":{\"x\":520,\"y\":90,\"text\":\"取值\",\"click\":\"button_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button1\",\"layoutConfig\":{},\"userConfig\":{\"x\":520,\"y\":130,\"text\":\"传值\",\"click\":\"button1_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button2\",\"layoutConfig\":{},\"userConfig\":{\"x\":520,\"y\":170,\"text\":\"clear\",\"click\":\"button2_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button3\",\"layoutConfig\":{},\"userConfig\":{\"x\":520,\"y\":210,\"text\":\"json\",\"click\":\"button3_click\"}},{\"cid\":\"vmd.ux.RichTextType\",\"id\":\"richtexttype\",\"layoutConfig\":{},\"userConfig\":{\"x\":100,\"y\":20}}]},{\"cid\":\"vmddataset\",\"id\":\"数据集\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdvariable\",\"id\":\"变量\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdsubviewport\",\"id\":\"SubView_Window\",\"layoutConfig\":{},\"userConfig\":{}}]}","vmdevents":"function cellRichTextType() {\n    this.allowEdit = {\n            id: \"allowEdit\",\n            value: \"false\",\n            type: \"CheckBox\"\n        },\n        this.allowPrint = {\n            id: \"allowPrint\",\n            value: \"false\",\n            type: \"CheckBox\"\n        },\n        this.allowEmpty = {\n            id: \"allowEmpty\",\n            value: \"false\",\n            type: \"CheckBox\"\n        },\n        this.indent = {\n            id: \"indent\",\n            value: \"0\",\n            type: \"Text\"\n        },\n        this.emptyAlert = {\n            id: \"emptyAlert\",\n            value: \"\",\n            type: \"Text\"\n        }\n\n    //清空属性\n    this.clearInfo = function() {\n        this.allowEdit.value = \"false\"\n        this.allowPrint.value = \"false\";\n        this.allowEmpty.value = \"false\"\n        this.indent.value = \"0\";\n        this.emptyAlert.value = \"\";\n    };\n\n    //设置值\n    this.setCellTextInfo = function(info, cell) {\n        this.allowEdit.value = info.allowEdit.value\n        this.allowPrint.value = info.allowPrint.value\n        this.allowEmpty.value = info.allowEmpty.value\n        this.indent.value = info.indent.value\n        this.emptyAlert.value = info.emptyAlert.value\n    };\n\n    // 根据id设置相应属性的值\n    this.setCellInfos = function(id, value) {\n        var c = this.getInfoByID(id);\n        if(c) {\n            if(c.type == \"Text\") {\n                c.value = value;\n            }\n            if(c.type == \"CheckBox\") {\n                c.checked = value;\n            }\n            if(c.type == \"ComboBox\") {\n                c.value = value;\n            }\n            if(c.type == \"compound\") {\n                c.value = value;\n            }\n            if(c.type == \"comlist\") {\n                c.value = value;\n            }\n            if(c.type == \"comlist\") {\n                c.value = value;\n            }\n            if(c.type == \"radiostoregroup\") {\n                c.value = value;\n            }\n            if(c.type == \"ColorSelect\") {\n                c.value = value;\n            }\n            if(c.type == \"LineStyle\") {\n                c.value = value;\n            }\n        }\n    };\n\n    //通过id获取属性\n    this.getInfoByID = function(id) {\n        if(this.allowEdit.id == id) {\n            return this.allowEdit;\n        }\n        if(this.allowPrint.id == id) {\n            return this.allowPrint;\n        }\n        if(this.allowEmpty.id == id) {\n            return this.allowEmpty;\n        }\n        if(this.indent.id == id) {\n            return this.indent;\n        }\n        if(this.emptyAlert.id == id) {\n            return this.emptyAlert;\n        }\n    }\n\n    //转换JSON串\n    this.cellInfoToJson = function() {\n        json = {\n            cellRichTextInfo: [{\n                allowEdit: {\n                    id: 'allowEdit',\n                    value: this.allowEdit.value,\n                    type: 'CheckBox'\n                },\n                allowPrint: {\n                    id: 'allowPrint',\n                    value: this.allowPrint.value,\n                    type: 'CheckBox'\n                },\n                allowEmpty: {\n                    id: 'allowEmpty',\n                    value: this.allowEmpty.value,\n                    type: 'CheckBox'\n                },\n                indent: {\n                    id: 'indent',\n                    value: this.indent.value,\n                    type: 'Text'\n                },\n                emptyAlert: {\n                    id: 'emptyAlert',\n                    value: this.emptyAlert.value,\n                    type: 'Text'\n                }\n            }]\n        }\n    }\n}\nvar p = new cellRichTextType();\n\nfunction button_click(sender, e) {\n    p.allowEdit.value = \"true\"\n    p.allowPrint.value = \"false\";\n    p.allowEmpty.value = \"false\"\n    p.indent.value = \"10\";\n    p.emptyAlert.value = \"123\";\n    richtexttype.setInfo(p, null)\n}\n\nfunction button1_click(sender, e) {\n    p.allowEdit.value = richtexttype.getInfo(\"allowEdit\")\n    p.allowPrint.value = richtexttype.getInfo(\"allowPrint\")\n    p.allowEmpty.value = richtexttype.getInfo(\"allowEmpty\")\n    p.indent.value = richtexttype.getInfo(\"indent\")\n    p.emptyAlert.value = richtexttype.getInfo(\"emptyAlert\")\n    alert(p.allowEdit.value + \",\" + p.allowPrint.value + \",\" + p.allowEmpty.value + \",\" + p.indent.value + \",\" + p.emptyAlert.value)\n}\n\nfunction button2_click(sender, e) {\n    p.clearInfo()\n    richtexttype.setInfo(p, null)\n}\n\nfunction button3_click(sender, e) {\n    p.allowEdit.value = richtexttype.getInfo(\"allowEdit\")\n    p.allowPrint.value = richtexttype.getInfo(\"allowPrint\")\n    p.allowEmpty.value = richtexttype.getInfo(\"allowEmpty\")\n    p.indent.value = richtexttype.getInfo(\"indent\")\n    p.emptyAlert.value = richtexttype.getInfo(\"emptyAlert\")\n    p.cellInfoToJson()\n    alert(JSON.stringify(json))\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}