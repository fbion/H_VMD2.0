    c.value = value;\n            }\n            if(c.type == \"combo\") {\n                c.value = value;\n            }\n            if(c.type == \"radiostoregroup\") {\n                c.value = value;\n            }\n            if(c.type == \"ColorSelect\") {\n                c.value = value;\n            }\n            if(c.type == \"LineStyle\") {\n                c.value = value;\n            }\n        }\n    };\n\n    //通过id获取属性\n    this.getInfoByID = function(id) {\n        if(this.allowEdit.id == id) {\n            return this.allowEdit;\n        }\n        if(this.allowPrint.id == id) {\n            return this.allowPrint;\n        }\n        if(this.allowEmpty.id == id) {\n            return this.allowEmpty;\n        }\n        if(this.allowFloat.id == id) {\n            return this.allowFloat;\n        }\n        if(this.allowNegative.id == id) {\n            return this.allowNegative;\n        }\n        if(this.max.id == id) {\n            return this.max;\n        }\n        if(this.min.id == id) {\n            return this.min;\n        }\n\n\n        if(this.limit.id == id) {\n            return this.limit;\n        }\n        if(this.decimalPoints.id == id) {\n            return this.decimalPoints;\n        }\n        if(this.emptyAlert.id == id) {\n            return this.emptyAlert;\n        }\n\n\n\n    }\n\n    //转换JSON串\n    this.cellInfoToJson = function() {\n        json = {\n            cellNumberInfo: [{\n                allowEdit: {\n                    id: 'allowEdit',\n                    value: this.allowEdit.value,\n                    type: 'CheckBox'\n                },\n                allowPrint: {\n                    id: 'allowPrint',\n                    value: this.allowPrint.value,\n                    type: 'CheckBox'\n                },\n                allowEmpty: {\n                    id: 'allowEmpty',\n                    value: this.allowEmpty.value,\n                    type: 'CheckBox'\n                },\n                allowFloat: {\n                    id: 'allowFloat',\n                    value: this.allowFloat.value,\n                    type: 'CheckBox'\n                },\n                allowNegative: {\n                    id: 'allowNegative',\n                    value: this.allowNegative.value,\n                    type: 'CheckBox'\n                },\n                max: {\n                    id: 'max',\n                    value: this.max.value,\n                    type: 'CheckBox'\n                },\n                min: {\n                    id: 'min',\n                    value: this.min.value,\n                    type: 'CheckBox'\n                },\n                limit: {\n                    id: 'limit',\n                    value: this.limit.value,\n                    type: 'CheckBox'\n                },\n                decimalPoints: {\n                    id: 'decimalPoints',\n                    value: this.decimalPoints.value,\n                    type: 'Text'\n                },\n                emptyAlert: {\n                    id: 'emptyAlert',\n                    value: this.emptyAlert.value,\n                    type: 'Text'\n                }\n\n\n\n\n\n            }]\n\n        }\n    }\n}\nvar p = new cellNumberType()\n\nfunction button_click(sender, e) {\n    p.allowEmpty.value = NumberType.getInfo(\"allowEmpty\")\n    p.allowEdit.value = NumberType.getInfo(\"allowEdit\")\n    p.allowPrint.value = NumberType.getInfo(\"allowPrint\")\n    p.allowFloat.value = NumberType.getInfo(\"allowFloat\")\n    p.allowNegative.value = NumberType.getInfo(\"allowNegative\")\n    p.max.value = NumberType.getInfo(\"max\")\n    p.min.value = NumberType.getInfo(\"min\")\n    p.limit.value = NumberType.getInfo(\"limit\")\n    p.decimalPoints.value = NumberType.getInfo(\"decimalPoints\")\n    p.emptyAlert.value = NumberType.getInfo(\"emptyAlert\")\n\n    alert(p.allowEmpty.value + \",\" + p.allowEdit.value + \",\" + p.allowPrint.value +\n        \",\" + p.allowFloat.value + \",\" + p.allowNegative.value + \",\" + p.max.value +\n        \",\" + p.min.value + \",\" + p.limit.value + \",\" + p.decimalPoints.value + \",\" + p.emptyAlert.value)\n}\n\nfunction button1_click(sender, e) {\n    p.allowEmpty.value = \"false\"\n    p.allowEdit.value = \"false\"\n    p.allowPrint.value = \"false\"\n    p.allowFloat.value = \"false\"\n    p.allowNegative.value = \"false\"\n    p.max.value = \"false\"\n    p.min.value = \"false\"\n    p.limit.value = \"false\"\n    p.decimalPoints.value = \"4\"\n    p.emptyAlert.value = \"123\"\n    NumberType.setInfo(p, null)\n}\n\nfunction button2_click(sender, e) {\n    p.clearInfo()\n    NumberType.setInfo(p, null)\n}\n\nfunction button3_click(sender, e) {\n    p.allowEmpty.value = NumberType.getInfo(\"allowEmpty\")\n    p.allowEdit.value = NumberType.getInfo(\"allowEdit\")\n    p.allowPrint.value = NumberType.getInfo(\"allowPrint\")\n    p.allowFloat.value = NumberType.getInfo(\"allowFloat\")\n    p.allowNegative.value = NumberType.getInfo(\"allowNegative\")\n    p.max.value = NumberType.getInfo(\"max\")\n    p.min.value = NumberType.getInfo(\"min\")\n    p.limit.value = NumberType.getInfo(\"limit\")\n    p.decimalPoints.value = NumberType.getInfo(\"decimalPoints\")\n    p.emptyAlert.value = NumberType.getInfo(\"emptyAlert\")\n    p.cellInfoToJson()\n    alert(JSON.stringify(json))\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}