{"vmdversion":"2.0.3.0907","vmdlayout":"{\"components\":[{\"cid\":\"viewport\",\"id\":\"MainViewport\",\"layoutConfig\":{},\"userConfig\":{\"layout\":\"absolute\"},\"cn\":[{\"cid\":\"vmd.ux.RadioButtonType\",\"id\":\"RadioButtonType\",\"layoutConfig\":{},\"userConfig\":{\"x\":50,\"y\":20}},{\"cid\":\"vmdButton\",\"id\":\"button\",\"layoutConfig\":{},\"userConfig\":{\"x\":570,\"y\":80,\"text\":\"取值\",\"click\":\"button_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button1\",\"layoutConfig\":{},\"userConfig\":{\"x\":570,\"y\":120,\"text\":\"传值\",\"click\":\"button1_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button2\",\"layoutConfig\":{},\"userConfig\":{\"x\":570,\"y\":160,\"text\":\"clear\",\"click\":\"button2_click\"}},{\"cid\":\"vmdButton\",\"id\":\"button3\",\"layoutConfig\":{},\"userConfig\":{\"x\":570,\"y\":200,\"text\":\"json\",\"click\":\"button3_click\"}}]},{\"cid\":\"vmddataset\",\"id\":\"数据集\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdvariable\",\"id\":\"变量\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdsubviewport\",\"id\":\"SubView_Window\",\"layoutConfig\":{},\"userConfig\":{}}]}","vmdevents":"function cellRadioButtonType() {\r\n    this.allowEdit = {\r\n            id: \"allowEdit\",\r\n            value: \"true\",\r\n            type: \"CheckBox\"\r\n        },\r\n        this.allowPrint = {\r\n            id: \"allowPrint\",\r\n            value: \"true\",\r\n            type: \"CheckBox\"\r\n        },\r\n        this.auto = {\r\n            id: \"auto\",\r\n            value: \"false\",\r\n            type: \"CheckBox\"\r\n        },\r\n        this.displayRows = {\r\n            id: \"displayRows\",\r\n            value: \"0\",\r\n            type: \"Text\"\r\n        },\r\n        this.rowMargin = {\r\n            id: \"rowMargin\",\r\n            value: \"0\",\r\n            type: \"Text\"\r\n        },\r\n        this.displayType = {\r\n            id: \"displayType\",\r\n            value: \"round\",\r\n            type: \"radiostoregroup\"\r\n        }\r\n\r\n\r\n\r\n\r\n    //清空属性\r\n    this.clearInfo = function() {\r\n        this.allowEdit.value = \"true\"\r\n        this.allowPrint.value = \"true\"\r\n        this.auto.value = \"false\"\r\n        this.displayRows.value = \"0\"\r\n        this.rowMargin.value = \"0\"\r\n        this.displayType.value = \"round\"\r\n    };\r\n\r\n    //设置值\r\n    this.setCellTextInfo = function(info, cell) {\r\n        this.allowEdit.value = info.allowEdit.value\r\n        this.allowPrint.value = info.allowPrint.value\r\n        this.auto.value = info.auto.value\r\n        this.displayRows.value  = info.displayRows.value\r\n        this.rowMargin.value  = info.rowMargin.value\r\n        this.displayType.value  = info.displayType.value\r\n    };\r\n\r\n    // 根据id设置相应属性的值\r\n    this.setCellInfos = function(id, value) {\r\n        var c = this.getInfoByID(id);\r\n        if(c) {\r\n            if(c.type == \"Text\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"CheckBox\") {\r\n                c.checked = value;\r\n            }\r\n            if(c.type == \"ComboBox\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"compound\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"combo\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"radiostoregroup\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"ColorSelect\") {\r\n                c.value = value;\r\n            }\r\n            if(c.type == \"LineStyle\") {\r\n                c.value = value;\r\n            }\r\n        }\r\n    };\r\n\r\n    //通过id获取属性\r\n    this.getInfoByID = function(id) {\r\n        if(this.allowEdit.id == id) {\r\n            return this.allowEdit;\r\n        }\r\n        if(this.allowPrint.id == id) {\r\n            return this.allowPrint;\r\n        }\r\n        if(this.auto.id == id) {\r\n            return this.auto;\r\n        }\r\n        if(this.displayRows.id == id) {\r\n            return this.displayRows;\r\n        }\r\n        if(this.rowMargin.id == id) {\r\n            return this.rowMargin;\r\n        }\r\n        if(this.displayType.id == id) {\r\n            return this.displayType;\r\n        }\r\n\r\n    }\r\n\r\n    //转换JSON串\r\n    this.cellInfoToJson = function() {\r\n        json = {\r\n            cellNumberInfo: [{\r\n                allowEdit: {\r\n                    id: 'allowEdit',\r\n                    value: this.allowEdit.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                allowPrint: {\r\n                    id: 'allowPrint',\r\n                    value: this.allowPrint.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                auto: {\r\n                    id: 'auto',\r\n                    value: this.auto.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                displayRows: {\r\n                    id: 'displayRows',\r\n                    value: this.displayRows.value,\r\n                    type: 'Text'\r\n                },\r\n                rowMargin: {\r\n                    id: 'rowMargin',\r\n                    value: this.rowMargin.value,\r\n                    type: 'Text'\r\n                },\r\n                displayType: {\r\n                    id: 'displayType',\r\n                    value: this.displayType.value,\r\n                    type: 'radiostoregroup'\r\n                }\r\n            }]\r\n\r\n        }\r\n    }\r\n}\r\n\r\n\r\nvar p = new cellRadioButtonType();\r\n\r\n\r\nfunction button_click(sender, e) {\r\n    p.allowEdit.value = \"true\"\r\n    p.allowPrint.value = \"false\"\r\n    p.auto.value = \"false\"\r\n    p.displayRows.value = \"15\"\r\n    p.rowMargin.value = \"78\"\r\n    p.displayType.value = \"square\"\r\n    RadioButtonType.setInfo(p, null)\r\n}\r\n\r\nfunction button1_click(sender, e) {\r\n    p.allowEdit.value = RadioButtonType.getInfo(\"allowEdit\")\r\n    p.allowPrint.value = RadioButtonType.getInfo(\"allowPrint\")\r\n    p.auto.value = RadioButtonType.getInfo(\"auto\")\r\n    p.displayRows.value = RadioButtonType.getInfo(\"displayRows\")\r\n    p.rowMargin.value = RadioButtonType.getInfo(\"rowMargin\")\r\n    p.displayType.value = RadioButtonType.getInfo(\"displayType\")\r\n    alert(p.allowEdit.value + \",\" + p.allowPrint.value + \",\" + p.auto.value +\r\n        \",\" + p.displayRows.value + \",\" + p.rowMargin.value + \",\" + p.displayType.value)\r\n}\r\n\r\nfunction button2_click(sender, e) {\r\n    p.clearInfo()\r\n    RadioButtonType.setInfo(p, null)\r\n}\r\n\r\nfunction button3_click(sender, e) {\r\n    p.allowEdit.value = RadioButtonType.getInfo(\"allowEdit\")\r\n    p.allowPrint.value = RadioButtonType.getInfo(\"allowPrint\")\r\n    p.auto.value = RadioButtonType.getInfo(\"auto\")\r\n    p.displayRows.value = RadioButtonType.getInfo(\"displayRows\")\r\n    p.rowMargin.value = RadioButtonType.getInfo(\"rowMargin\")\r\n    p.displayType.value = RadioButtonType.getInfo(\"displayType\")\r\n    p.cellInfoToJson()\r\n    alert(JSON.stringify(json))\r\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}