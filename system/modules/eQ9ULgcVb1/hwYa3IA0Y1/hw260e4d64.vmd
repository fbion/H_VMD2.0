   this.clearInfo = function() {\n        this.typeSetting.value = \"0\"\n        this.dataSet.value = \"\"\n        this.saveFiled.value = \"\"\n        this.myDisplayFiled.value = \"\"\n        this.dropDownDisplayColumn.value = \"\"\n        this.filterCondition.value = \"\"\n    };\n\n    //设置值\n    this.setCellDataInfo = function(info, cell) {\n        this.typeSetting.value = info.typeSetting.value\n        this.dataSet.value = info.dataSet.value\n        this.saveFiled.value = info.saveFiled.value\n        this.myDisplayFiled.value = info.myDisplayFiled.value\n        this.dropDownDisplayColumn.value = info.dropDownDisplayColumn.value\n        this.filterCondition.value = info.filterCondition.value\n    };\n\n    // 根据id设置相应属性的值\n    this.setCellInfos = function(id, value) {\n        var c = this.getInfoByID(id);\n        if(c) {\n            if(c.type == \"Text\") {\n                c.value = value;\n            }\n            if(c.type == \"CheckBox\") {\n                c.checked = value;\n            }\n            if(c.type == \"ComboBox\") {\n                c.value = value;\n            }\n            if(c.type == \"compound\") {\n                c.value = value;\n            }\n            if(c.type == \"combo\") {\n                c.value = value;\n            }\n            if(c.type == \"radiostoregroup\") {\n                c.value = value;\n            }\n            if(c.type == \"ColorSelect\") {\n                c.value = value;\n            }\n            if(c.type == \"LineStyle\") {\n                c.value = value;\n            }\n        }\n    };\n\n    //通过id获取属性\n    this.getInfoByID = function(id) {\n        if(this.typeSetting.id == id) {\n            return this.typeSetting;\n        }\n        if(this.dataSet.id == id) {\n            return this.dataSet;\n        }\n        if(this.saveFiled.id == id) {\n            return this.saveFiled;\n        }\n        if(this.myDisplayFiled.id == id) {\n            return this.myDisplayFiled;\n        }\n        if(this.dropDownDisplayColumn.id == id) {\n            return this.dropDownDisplayColumn;\n        }\n        if(this.filterCondition.id == id) {\n            return this.filterCondition;\n        }\n    }\n\n    //转换JSON串\n    this.cellInfoToJson = function() {\n        json = {\n            cellDataInfo: [{\n                typeSetting: {\n                    id: 'typeSetting',\n                    value: this.typeSetting.value,\n                    type: 'comlist'\n                },\n                dataSet: {\n                    id: 'dataSet',\n                    value: this.dataSet.value,\n                    type: 'comlist'\n                },\n                saveFiled: {\n                    id: 'saveFiled',\n                    value: this.saveFiled.value,\n                    type: 'comlist'\n                },\n                myDisplayFiled: {\n                    id: 'myDisplayFiled',\n                    value: this.myDisplayFiled.value,\n                    type: 'comlist'\n                },\n                dropDownDisplayColumn: {\n                    id: 'dropDownDisplayColumn',\n                    value: this.dropDownDisplayColumn.value,\n                    type: 'Text'\n                },\n                filterCondition: {\n                    id: 'filterCondition',\n                    value: this.filterCondition.value,\n                    type: 'Text'\n                }\n            }]\n\n        }\n    }\n}\nvar p = new cellOrdinaryDataSetType()\n\nfunction button_click(sender, e) {\n    p.typeSetting.value = \"1\"\n    p.dataSet.value = \"\"\n    p.saveFiled.value = \"\"\n    p.myDisplayFiled.value = \"\"\n    p.dropDownDisplayColumn.value = \"11\"\n    p.filterCondition.value = \"11\"\n    OrdinaryDataSet.setInfo(p, null)\n}\n\nfunction button1_click(sender, e) {\n    p.typeSetting.value = OrdinaryDataSet.getInfo(\"typeSetting\")\n    p.dataSet.value = OrdinaryDataSet.getInfo(\"dataSet\")\n    p.saveFiled.value = OrdinaryDataSet.getInfo(\"saveFiled\")\n    p.myDisplayFiled.value = OrdinaryDataSet.getInfo(\"myDisplayFiled\")\n    p.dropDownDisplayColumn.value = OrdinaryDataSet.getInfo(\"dropDownDisplayColumn\")\n    p.filterCondition.value = OrdinaryDataSet.getInfo(\"filterCondition\")\n    alert(p.typeSetting.value + \",\" + p.dataSet.value + \",\" + p.saveFiled.value +\n        \",\" + p.myDisplayFiled.value + \",\" + p.filterCondition.value + \",\" + p.dropDownDisplayColumn.value)\n}\n\nfunction button2_click(sender, e) {\n    p.clearInfo()\n    OrdinaryDataSet.setInfo(p, null)\n}\n\nfunction button3_click(sender, e) {\n    p.typeSetting.value = OrdinaryDataSet.getInfo(\"typeSetting\")\n    p.dataSet.value = OrdinaryDataSet.getInfo(\"dataSet\")\n    p.saveFiled.value = OrdinaryDataSet.getInfo(\"saveFiled\")\n    p.myDisplayFiled.value = OrdinaryDataSet.getInfo(\"myDisplayFiled\")\n    p.dropDownDisplayColumn.value = OrdinaryDataSet.getInfo(\"dropDownDisplayColumn\")\n    p.filterCondition.value = OrdinaryDataSet.getInfo(\"filterCondition\")\n    p.cellInfoToJson()\n    alert(JSON.stringify(json))\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}