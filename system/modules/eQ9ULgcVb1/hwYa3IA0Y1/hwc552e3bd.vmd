lowEdit.id == id) {\r\n            return this.allowEdit;\r\n        }\r\n        if(this.allowPrint.id == id) {\r\n            return this.allowPrint;\r\n        }\r\n        if(this.allowEmpty.id == id) {\r\n            return this.allowEmpty;\r\n        }\r\n        if(this.myWidth.id == id) {\r\n            return this.myWidth;\r\n        }\r\n        if(this.multiple.id == id) {\r\n            return this.multiple;\r\n        }\r\n        if(this.oneClick.id == id) {\r\n            return this.oneClick;\r\n        }\r\n        if(this.emptyAlert.id == id) {\r\n            return this.emptyAlert;\r\n        }\r\n\r\n\r\n\r\n    }\r\n\r\n    //转换JSON串\r\n    this.cellInfoToJson = function() {\r\n        json = {\r\n            cellNumberInfo: [{\r\n                allowEdit: {\r\n                    id: 'allowEdit',\r\n                    value: this.allowEdit.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                allowPrint: {\r\n                    id: 'allowPrint',\r\n                    value: this.allowPrint.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                allowEmpty: {\r\n                    id: 'allowEmpty',\r\n                    value: this.allowEmpty.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                emptyAlert: {\r\n                    id: 'emptyAlert',\r\n                    value: this.emptyAlert.value,\r\n                    type: 'Text'\r\n                },\r\n                multiple: {\r\n                    id: 'multiple',\r\n                    value: this.multiple.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                oneClick: {\r\n                    id: 'oneClick',\r\n                    value: this.oneClick.value,\r\n                    type: 'CheckBox'\r\n                },\r\n                myWidth: {\r\n                    id: 'myWidth',\r\n                    value: this.myWidth.value,\r\n                    type: 'Text'\r\n                }\r\n\r\n\r\n\r\n\r\n\r\n            }]\r\n\r\n        }\r\n    }\r\n}\r\nvar p = new cellComboType()\r\n\r\nfunction button_click(sender, e) {\r\n    p.allowEmpty.value = \"false\"\r\n    p.allowEdit.value = \"false\"\r\n    p.allowPrint.value = \"false\"\r\n    p.emptyAlert.value = \"false\"\r\n    p.myWidth.value = \"false\"\r\n    p.multiple.value = \"false\"\r\n    p.oneClick.value = \"false\"\r\n    ComboType.setInfo(p, null)\r\n}\r\n\r\nfunction button1_click(sender, e) {\r\n    p.allowEmpty.value = ComboType.getInfo(\"allowEmpty\")\r\n    p.allowEdit.value = ComboType.getInfo(\"allowEdit\")\r\n    p.allowPrint.value = ComboType.getInfo(\"allowPrint\")\r\n    p.emptyAlert.value = ComboType.getInfo(\"emptyAlert\")\r\n    p.myWidth.value = ComboType.getInfo(\"myWidth\")\r\n    p.multiple.value = ComboType.getInfo(\"multiple\")\r\n    p.oneClick.value = ComboType.getInfo(\"oneClick\")\r\n    alert(p.allowEmpty.value+\",\"+p.allowEdit.value+\",\"+p.allowPrint.value\r\n    +\",\"+p.emptyAlert.value+\",\"+p.myWidth.value+\",\"+p.multiple.value+\",\"+p.oneClick.value)\r\n}\r\n\r\nfunction button2_click(sender,e){\r\np.clearInfo()\r\nComboType.setInfo(p,null)\r\n}\r\n\r\nfunction button3_click(sender,e){\r\np.allowEmpty.value = ComboType.getInfo(\"allowEmpty\")\r\n    p.allowEdit.value = ComboType.getInfo(\"allowEdit\")\r\n    p.allowPrint.value = ComboType.getInfo(\"allowPrint\")\r\n    p.emptyAlert.value = ComboType.getInfo(\"emptyAlert\")\r\n    p.myWidth.value = ComboType.getInfo(\"myWidth\")\r\n    p.multiple.value = ComboType.getInfo(\"multiple\")\r\n    p.oneClick.value = ComboType.getInfo(\"oneClick\")\r\n    p.cellInfoToJson()\r\n    alert(JSON.stringify(json))\r\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"