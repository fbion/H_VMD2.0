lue = info.myDisplayFiled.value\n        this.myFatherFiled.value = info.myFatherFiled.value\n        this.myNodeFiled.value = info.myNodeFiled.value\n        this.rootValue.value = info.rootValue.value\n        this.rootNodeText.value = info.rootNodeText.value\n        this.myValueFiled.value = info.myValueFiled.value\n    };\n\n    // 根据id设置相应属性的值\n    this.setCellInfos = function(id, value) {\n        var c = this.getInfoByID(id);\n        if(c) {\n            if(c.type == \"Text\") {\n                c.value = value;\n            }\n            if(c.type == \"CheckBox\") {\n                c.checked = value;\n            }\n            if(c.type == \"ComboBox\") {\n                c.value = value;\n            }\n            if(c.type == \"compound\") {\n                c.value = value;\n            }\n            if(c.type == \"combo\") {\n                c.value = value;\n            }\n            if(c.type == \"radiostoregroup\") {\n                c.value = value;\n            }\n            if(c.type == \"ColorSelect\") {\n                c.value = value;\n            }\n            if(c.type == \"LineStyle\") {\n                c.value = value;\n            }\n        }\n    };\n\n    //通过id获取属性\n    this.getInfoByID = function(id) {\n        if(this.dataSet.id == id) {\n            return this.dataSet;\n        }\n        if(this.myDisplayFiled.id == id) {\n            return this.myDisplayFiled;\n        }\n        if(this.myFatherFiled.id == id) {\n            return this.myFatherFiled;\n        }\n        if(this.myNodeFiled.id == id) {\n            return this.myNodeFiled;\n        }\n        if(this.rootValue.id == id) {\n            return this.rootValue;\n        }\n        if(this.rootNodeText.id == id) {\n            return this.rootNodeText;\n        }\n        if(this.myValueFiled.id == id) {\n            return this.myValueFiled;\n        }\n    }\n\n    //转换JSON串\n    this.cellInfoToJson = function() {\n        json = {\n            cellDropDownTreeDataInfo: [{\n                dataSet: {\n                    id: 'dataSet',\n                    value: this.dataSet.value,\n                    type: 'comlist'\n                },\n                myDisplayFiled: {\n                    id: 'myDisplayFiled',\n                    value: this.myDisplayFiled.value,\n                    type: 'comlist'\n                },\n                myFatherFiled: {\n                    id: 'myFatherFiled',\n                    value: this.myFatherFiled.value,\n                    type: 'comlist'\n                },\n                myNodeFiled: {\n                    id: 'myNodeFiled',\n                    value: this.myNodeFiled.value,\n                    type: 'comlist'\n                },\n                rootValue: {\n                    id: 'rootValue',\n                    value: this.rootValue.value,\n                    type: 'comlist'\n                },\n                rootNodeText: {\n                    id: 'rootNodeText',\n                    value: this.rootNodeText.value,\n                    type: 'comlist'\n                },\n                myValueFiled: {\n                    id: 'myValueFiled',\n                    value: this.myValueFiled.value,\n                    type: 'comlist'\n                }\n            }]\n\n        }\n    }\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}