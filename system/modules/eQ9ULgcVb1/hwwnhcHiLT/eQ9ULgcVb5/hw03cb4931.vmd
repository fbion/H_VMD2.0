is;\n    white-space: nowrap;\n    overflow: hidden;\n    padding-right: 5px;\n    -webkit-flex: 1;\n    /* Chrome */\n    -ms-flex: 1;\n    /* IE 10 */\n    flex: 1;\n}\n\n.info a {\n    text-decoration: none;\n    color: #333;\n}\n\n.info .rq {\n    /*position: absolute;*/\n    right: 0px;\n    top: 0;\n    color: #333;\n    font-size: 13px;\n    width: 140px;\n    text-align: right;\n    padding-right: 20px;\n}\n\nli.info-hover {\n    cursor: pointer\n}\n\nli.info-hover a {\n    color: #1868b8\n}\n\n.d-info {\n    padding: 8px 0;\n}\n\n.header .more:hover {\n    color: #1868b8;\n}","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}