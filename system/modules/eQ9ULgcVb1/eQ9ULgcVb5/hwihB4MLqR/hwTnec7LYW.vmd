tore({\n    proxy: new Ext.data.MemoryProxy(),\n    fields: ['name', 'value']\n});\nvar data1 = [{\n    name: '横向',\n    value: '横向'\n}, {\n    name: '纵向',\n    value: '纵向'\n}];\nstore1.loadData(data1);\n\nfunction combo_beforerender(sender) {\n    sender.store = store;\n    sender.displayField = 'name'\n    sender.valueField = 'value'\n}\n\nfunction cmb_drecition_beforerender(sender) {\n    sender.store = store1;\n    sender.displayField = 'name'\n    sender.valueField = 'value'\n}\n\n\nfunction MainViewport_afterrender(sender) {\n    rpt = parent.selectRptPrint;\n}\n\nfunction cmb_size_afterrender(sender) {\n    cmb_size.setValue(\"A4\");\n}\n\nfunction cmb_drecition_afterrender(sender) {\n    cmb_drecition.setValue(\"横向\");\n}\n\nfunction btn_ok_click(sender, e) {\n    parent.closePrintWindow();\n    rpt.print(options);\n}\n\nfunction btn_cansel_click(sender, e) {\n    parent.closePrintWindow();\n}\n\nfunction cmb_size_change(sender, value, text) {\n    if (options)\n        options.pageSize = value;\n}\n\nfunction cmb_drecition_change(sender, value, text) {\n    if (options) {\n        if (value == \"横向\") {\n            options.pageDirection = 0;\n        } else {\n            options.pageDirection = 1;\n        }\n    }\n\n}\n\nfunction hnb_mass_beforerender(sender) {\n    hnb_mass.on(\"change\", function() {\n        if (options)\n            options.dpihStandard = arguments[1];\n    })\n}\n\nfunction hnb_tpadding_beforerender(sender) {\n    hnb_tpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.top = arguments[1];\n    })\n}\n\nfunction hnb_bpadding_beforerender(sender) {\n    hnb_bpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.bottom = arguments[1];\n    })\n}\n\nfunction hnb_lpadding_beforerender(sender) {\n    hnb_lpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.left = arguments[1];\n    })\n}\n\nfunction hnb_rpadding_beforerender(sender) {\n    hnb_rpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.right = arguments[1];\n    })\n}\n\nfunction hnb_hpadding_beforerender(sender) {\n    hnb_hpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.header = arguments[1];\n    })\n}\n\nfunction hnb_fpadding_beforerender(sender) {\n    hnb_fpadding.on(\"change\", function() {\n        if (options.pageMargins)\n            options.pageMargins.footer = arguments[1];\n    })\n}\n\nfunction hwCheckbox_check(sender, checked) {\n    if (options)\n        options.repeatHeader = checked;\n}","vmdcss":"","vmdprops":"\"\"","type":"module","vmdresource":"{\"components\":[{\"cid\":\"vmdrescsss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresjss\",\"layoutConfig\":{},\"userConfig\":{}},{\"cid\":\"vmdresimgs\",\"layoutConfig\":{},\"userConfig\":{}}]}"}